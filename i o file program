#include <stdio.h>
#include <stdlib.h>

int main(){
FILE*fp;
fp=fopen("data.txt","a");//Open file in in append mode
if(fp==NULL){
    printf("File cannot be opened!\n");
    return 1;
}
fprintf(fp,"thise line is appended at the end .\n");
fclose(fp);
printf("Data appended successfully.\n");
return 0;

}
